{
    "env": {
        "node": true,
        "browser": true,
        "es6": true
    },
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": { 
        "react/jsx-filename-extension": [ 1, { "extensions": [ ".js", ".jsx" ] } ],
        "react/prop-types": [0, { "ignore": 0, "customValidators": 0, "skipUndeclared": 0 }]
    }
}